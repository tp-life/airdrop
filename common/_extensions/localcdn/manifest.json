
{
    "version": "1.0.0",
    "manifest_version": 2,
    "name": "Chrome Fiddler",
    "permissions": [
        "proxy",
        "tabs",
        "unlimitedStorage",
        "storage",
        "<all_urls>",
        "webRequest",
        "webRequestBlocking",
		"cookies",
        "http://*/*",
        "https://*/*",
        "http://192.168.88.130/*"  // 添加局域网地址的权限
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": true
	},
    "minimum_chrome_version":"22.0.0"
}